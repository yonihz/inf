#
# gcc compliation aliases: c89, c99, debug mode, release mode
alias gd='gcc -ansi -pedantic-errors -Wall _Wextra -g'
alias gc='gcc -ansi -pedantic-errors -Wall _Wextra -DNDEBUG -03'
alias gd9='gcc -std=c99 -pedantic-errors -Wall _Wextra -g'
alias gc9='gcc -std=c99 -pedantic-errors -Wall _Wextra -DNDEBUG -03'
#
# Valgrind alias
alias vlg='valgrind --leak-check=yes -track-origins=yes'
